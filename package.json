{
  "name": "hello-world-server",
  "private": true,
  "version": "1.0.0",
  "description": "Server for HelloWorld!<>",
  "main": "config.js",
  "scripts": {
    "test": "xo",
    "lint": "xo",
    "format": "xo --fix",
    "start": "node ./bin/start.js",
    "dev": "nodemon ./bin/start.js",
    "migrate": "node ./bin/migrate.js",
    "migrate:create": "migrate create --migrations-dir='./src/migrations'"
  },
  "keywords": [
    "express",
    "postgres"
  ],
  "author": "Hugo Di Francesco",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "client-sessions": "^0.8.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "helmet": "^3.23.2",
    "migrate": "^1.6.2",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "sql-template-strings": "^2.2.2",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "xo": "^0.32.0"
  },
  "xo": {
    "prettier": true,
    "space": true
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "trailingComma": "none"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronsms/HelloWorldServer.git"
  },
  "bugs": {
    "url": "https://github.com/aaronsms/HelloWorldServer/issues"
  },
  "homepage": "https://github.com/aaronsms/HelloWorldServer#readme"
}
